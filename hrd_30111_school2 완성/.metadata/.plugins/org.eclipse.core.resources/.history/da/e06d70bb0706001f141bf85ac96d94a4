function add(){
	
	if(frm.sid.valuse.length == 0){
		alert('학번 입력 하세요');
		frm.sid.focus();
		return false;
	}
	else if(frm.sname.valuse.length == 0){
		alert('이름 입력 하세요');
		frm.sname.focus();
		return false;
	}
	else if(frm.midsocre.valuse.length == 0){
		alert('중간을 입력하세요');
		frm.midsocre.focus();
		return false;
	}
	else if(frm.dept_name.valuse.length == 0){
		alert('기말을 입력하세요');
		frm.dept_name.focus();
		return false;
	}
	else if(frm.phone.valuse.length == 0){
		alert('출석을 입력하세요');
		frm.phone.focus();
		return false;
	}
	else if(frm.email.valuse.length == 0){
		alert('이메일을 입력 하세요');
		frm.email.focus();
		return false;
	}
	else {
		alert("수강신청 완료")
		document.frm.submit();
		return ture;
		
	}





}