function add(){
	
	if(frm.sid.value.length == 0 || isNaN(frm.sname.value)){
		alert('학번 입력 하세요');
		frm.sid.focus();
		return false;
	}
	else if(frm.midsocre.value.length == 0 || isNaN(frm.sname.value)){
		alert('중간을 입력하세요');
		frm.midsocre.focus();
		return false;
	}
	else if(frm.finalscore.value.length == 0 || isNaN(frm.sname.value)){
		alert('기말을 입력하세요');
		frm.finalscore.focus();
		return false;
	}
	else if(frm.attend.value.length == 0 || isNaN(frm.sname.value)){
		alert('출석을 입력하세요');
		frm.attend.focus();
		return false;
	}
	else if(frm.report.value.length == 0 || isNaN(frm.sname.value)){
		alert('레포트를 입력하세요');
		frm.report.focus();
		return false;
	}
	else if(frm.etc.value.length == 0 || isNaN(frm.sname.value)){
		alert('기타를 입력하세요');
		frm.etc.focus();
		return false;
	}
	else {
		alert("수강신청 완료")
		document.frm.submit();
		return ture;
		
	}

}