CREATE TABLE TBL_CLASS_202201(
	resist_month varchar(6) not null PRIMARY KEY,
	c_no char(5) not null PRIMARY KEY,
	class_area varchar(15),
	tuition number(8),
	teacher_code char(3),
	
	primary key(resist_month, c_no);
)